{
  "ytdlp": {
    "path": "/usr/local/bin/yt-dlp",
    "options": {
      "format": "bestaudio[ext=m4a]/bestaudio",
      "geoBypass": true,
      "noCheckCertificate": true,
      "audioFormat": "m4a",
      "audioQuality": 0
    }
  },
  "mpv": {
    "path": "/usr/bin/mpv",
    "options": {
      "noVideo": true,
      "audioDevice": "auto",
      "volume": 100,
      "termOsdBar": true
    }
  }
}